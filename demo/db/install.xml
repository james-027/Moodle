<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/demo/db" VERSION="20200213" COMMENT="XMLDB file for local demo"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_demo" COMMENT="each record is one local demo">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="user_email" TYPE="text" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="user_name" TYPE="text" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="phone_number" TYPE="int" LENGTH="15" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="attachment" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
